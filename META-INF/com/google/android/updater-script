ui_print("@ Detecting Variant"); 
if is_substring("C701F", getprop("ro.boot.bootloader")) then
ui_print("@ Flashing C701F Kernel");
package_extract_file("sudo/kernel/c7pro/boot.img", file_getprop("/tmp/config", "boot"));endif;
if is_substring("C7010", getprop("ro.boot.bootloader")) then
if is_substring("c7proltedd", getprop("ro.product.device")) then    
ui_print("@ Flashing C7010 (OpenChina) Kernel");
package_extract_file("sudo/kernel/c7pro/boot.img", file_getprop("/tmp/config", "boot"));else
ui_print("@ Flashing C7010 (HongKong) Kernel");
package_extract_file("sudo/kernel/c7pro/boot.img", file_getprop("/tmp/config", "boot"));endif;endif;
if is_substring("C7018", getprop("ro.boot.bootloader")) then    
ui_print("@ Flashing C7018 (OpenChina) Kernel");
package_extract_file("sudo/kernel/c7pro/boot.img", file_getprop("/tmp/config", "boot"));endif;
if is_substring("C700", getprop("ro.boot.bootloader")) then
ui_print("@Flashing C7000 Kernel");
package_extract_file("sudo/kernel/c7/boot.img", file_getprop("/tmp/config", "boot"));endif;
