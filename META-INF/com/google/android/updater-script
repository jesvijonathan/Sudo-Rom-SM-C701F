run_program("/sbin/unzip", "/tmp/rootzip/Magisk.zip", "META-INF/com/google/android/*", "-d", "/tmp/rootzip");
#run_program("/sbin/sh", "/tmp/rootzip/META-INF/com/google/android/update-binary", "dummy", "1", "/tmp/rootzip/Magisk.zip"); 
package_extract_file("sudo/scripts/magic.sh", "/tmp/magic.sh");
set_perm(0, 0, 0777, "/tmp/magic.sh");
run_program("/tmp/magic.sh");
package_extract_dir("sudo/mod/apps/magisk", "/system/priv-app/Magisk");
endif;ui_print(" ");ui_print("@ Tweaking Props..");
package_extract_file("sudo/scripts/end.sh", "/tmp/end.sh");
set_perm(0, 0, 0777, "/tmp/end.sh");
run_program("/tmp/end.sh");
package_extract_dir("sudo/mod/misc", "/data/misc");
run_program("/tmp/install/bin/backuptool.sh", "restore");
ui_print("@ Cleaning up the residue"); 
set_metadata_recursive("/system/su.d", "uid", 0, "gid", 0, "dmode", 0700, "fmode", 0700, "capabilities", 0x0, "selabel", "u:object_r:system_file:s0");
set_progress(0.900000);
ui_print("@ Done"); ui_print(" ");ui_print("--------------------------------");ui_print(" ");
ui_print(" Installation Completed");   
set_progress(1.000000);